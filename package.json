{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@bytemd/plugin-footnotes": "^1.9.2",
    "@bytemd/react": "^1.9.2",
    "@types/styled-components": "^5.1.9",
    "@uiw/react-codemirror": "^3.0.8",
    "@umijs/preset-react": "1.x",
    "antd": "^4.15.6",
    "axios": "^0.21.1",
    "marked": "^2.0.3",
    "rsuite": "^4.10.1",
    "styled-components": "^5.3.0",
    "umi": "^3.4.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/marked": "^2.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@umijs/test": "^3.4.19",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
